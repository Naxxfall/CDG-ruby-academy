<h1>Posts</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Image</th>
    <th>Caption</th>
    <th>User</th>
    <th>Created_at</th>
    <th>Updated_at</th>
  </tr>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.id %></td>
        <td><%= image_tag post.image_url %></td>
        <td><%= post.caption %></td>
        <td><%= post.user_id %></td>
        <td><%= post.created_at %></td>
        <td><%= post.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>